'''
def f1(num1, num2):
    return num1 + num2

def f2(num1, num2, num3):
    return num1 + num2 + num3

# 시스템 변수, magic value
# __name__ 현재 호출한 파일의 이름을 갖는 변수
if __name__ == "__main__" :
    # f1과 f2를 테스트 하는 코드
    print(f1(10,20))
    print(f2(10,20,30))