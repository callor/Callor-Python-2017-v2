s = """
우리나라
대한민국
"""
print(s)

s = "우리나라\n대한민국"
print(s)

s1 = ["가나","나다","라마"]
j1 = "" . join(s1) # 문자열을 연결
print(j1)

j1 = ",".join(s1)
print(j1)


s1 = j1.split(",") # 컴마를 기준으로 분리
                # 결과물은 List
                
print(s1)

s1 = "Republic-Korea"
d1,d2,d3 = s1.partition("-")














